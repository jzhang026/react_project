  "main": "app.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "start": "rimraf build/* && yarn build:app:dev && concurrently --kill-others \"yarn webpack:watch:dev\" \"yarn watch:app:dev\"",
    "start:hot": "rimraf build/* && yarn build:app:dev && concurrently --kill-others \"yarn webpack:serve:dev\" \"yarn watch:app:dev\"",
    "build:app:dev": "cross-env NODE_ENV=development webpack --env development --config config/webpack.config.js",
    "watch:app:dev": "cross-env NODE_ENV=development nodemon build/server/server.js -i build/client",
    "webpack:serve:dev": "cross-env NODE_ENV=development webpack-serve --config config/webpack.config.dev.js --no-clipboard",
    "webpack:watch:dev": "cross-env NODE_ENV=development webpack --env development -w --config config/webpack.config.js"
  },
  "lint-staged": {
    "*.{js.css}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint src"
    ]
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-manifest-helpers": "^0.6.0",
    "history": "^4.10.1",
    "lint-staged": "^9.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.2.7",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
